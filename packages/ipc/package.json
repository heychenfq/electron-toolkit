{
  "name": "@electron-tools/ipc",
  "version": "1.0.0",
  "description": "electron ipc toolkit",
  "keywords": [
    "electron",
    "ipc"
  ],
  "author": "heychenfq <heychenfq@foxmail.com>",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "eslibc dev",
    "build": "eslibc clean && eslibc build"
  },
  "dependencies": {
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "eslibc": "workspace:*",
    "electron": "^18.3.3",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "eslibc": {
    "bundledTypes": [
      "rxjs"
    ],
    "esbuild": {
      "external": [
        "tslib",
        "tslib/*",
        "electron",
        "electron/*"
      ]
    }
  }
}
